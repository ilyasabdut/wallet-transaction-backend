    "@sentry/cli": "^2.33.0",
    "@sentry/nestjs": "^8.20.0",
    "@sentry/profiling-node": "^8.20.0",
    "bcrypt": "^5.1.1",
    "dotenv": "^16.4.5",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1",
    "swagger-ui-express": "^5.0.1"
